<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.yhb.ds2.mapper.Test1User2Mapper">

    <select id="findAll" resultType="cn.com.yhb.model.User">
      select
          id,
          name,
          age
      from user1
      where 1 = 1
    </select>

    <insert id="add" parameterType="cn.com.yhb.model.User">
        insert into user1 (name,age)
        values (#{name},#{age})
    </insert>

    <select id="queryById" parameterType="Integer" resultType="cn.com.yhb.model.User">
        select
          id,
          name,
          age
      from user1
      where id = #{id}
    </select>

</mapper>